{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"mocha": true
	},
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
	},
	"globals": {
		"require": false,
		"global": true
	},
	"rules": {
		"quotes": [2, "double"],
		"curly": [0],
		"no-mixed-requires": [0],
		"eol-last": [0],
		"no-trailing-spaces": [0],
		"no-multi-spaces": [0],
		"key-spacing": [0],
		"no-underscore-dangle": [0],
		"global-strict": [0],
		"react/jsx-uses-react": [1],
		"react/jsx-uses-vars": [1]
	}
}
